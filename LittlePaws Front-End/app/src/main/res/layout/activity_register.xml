<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="409dp"
    android:layout_height="match_parent"
    android:background="#EEE7FE"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbarFadeDuration="250"
        android:scrollbarSize="4dp"
        android:scrollbars="vertical">

        <ImageView
            android:id="@+id/paw2"
            android:layout_width="221dp"
            android:layout_height="216dp"
            android:rotation="340"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/paw" />

        <View
            android:id="@+id/phonenumtypebg"
            android:layout_width="326dp"
            android:layout_height="61dp"
            android:background="@drawable/logintypebg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/lastnametypebg"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/lastnametypebg"
            app:layout_constraintTop_toBottomOf="@+id/lastnametypebg"
            app:layout_constraintVertical_bias="0.08" />

        <TextView
            android:id="@+id/phonenum_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/italiana"
            android:text="Role"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/lastname_title"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/lastname_title"
            app:layout_constraintTop_toBottomOf="@+id/lastnametypebg"
            app:layout_constraintVertical_bias="0.024" />

        <EditText
            android:id="@+id/email_text2"
            android:layout_width="287dp"
            android:layout_height="48dp"
            android:layout_marginTop="3dp"
            android:backgroundTint="#852C42"
            android:ems="10"
            android:fontFamily="@font/italiana"
            android:hint="type here"
            android:inputType="text"
            android:textAlignment="textEnd"
            android:textColor="#FFFFFF"
            android:textColorHint="#D6D5D5"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/lastname_text"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/lastname_text"
            app:layout_constraintTop_toBottomOf="@+id/lastname_text"
            app:layout_constraintVertical_bias="0.086" />

        <TextView
            android:id="@+id/register_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:fontFamily="@font/italiana"
            android:text="Register"
            android:textAlignment="textEnd"
            android:textColor="#000000"
            android:textSize="48sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.753"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/login_button"
            android:layout_width="199dp"
            android:layout_height="61dp"
            android:layout_marginLeft="-6dp"
            android:layout_marginTop="-7dp"
            android:backgroundTint="#00FFFFFF"
            android:fontFamily="@font/italiana"
            android:text="you have an account? log-in here"
            android:textAlignment="textEnd"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="italic"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/register_title"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="@+id/register_title"
            app:layout_constraintTop_toBottomOf="@+id/register_title"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/submitlogin"
            android:layout_width="114dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:backgroundTint="#852C42"
            android:fontFamily="@font/italiana"
            android:text="Submit"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/passwordtypebg"
            app:layout_constraintHorizontal_bias="0.547"
            app:layout_constraintStart_toStartOf="@+id/passwordtypebg"
            app:layout_constraintTop_toBottomOf="@+id/passwordtypebg" />

        <TextView
            android:id="@+id/email_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/italiana"
            android:text="Email"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/phonenum_title"
            app:layout_constraintHorizontal_bias="0.989"
            app:layout_constraintStart_toStartOf="@+id/phonenum_title"
            app:layout_constraintTop_toBottomOf="@+id/lastnametypebg"
            app:layout_constraintVertical_bias="0.205" />

        <TextView
            android:id="@+id/lastname_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="79dp"
            android:fontFamily="@font/italiana"
            android:text="Last Name"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/lastnametypebg"
            app:layout_constraintEnd_toEndOf="@+id/firstname_title"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/firstname_title"
            app:layout_constraintTop_toBottomOf="@+id/firstname_title"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/firstname_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/italiana"
            android:text="First Name"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/login_button"
            app:layout_constraintHorizontal_bias="0.813"
            app:layout_constraintStart_toStartOf="@+id/login_button"
            app:layout_constraintTop_toBottomOf="@+id/login_button" />

        <View
            android:id="@+id/emailtypebg"
            android:layout_width="326dp"
            android:layout_height="61dp"
            android:background="@drawable/logintypebg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/phonenumtypebg"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/phonenumtypebg"
            app:layout_constraintTop_toBottomOf="@+id/phonenumtypebg"
            app:layout_constraintVertical_bias="0.096" />

        <View
            android:id="@+id/lastnametypebg"
            android:layout_width="326dp"
            android:layout_height="61dp"
            android:background="@drawable/logintypebg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/firstnametypebg"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/firstnametypebg"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.323" />

        <View
            android:id="@+id/firstnametypebg"
            android:layout_width="326dp"
            android:layout_height="61dp"
            android:background="@drawable/logintypebg"
            app:layout_constraintEnd_toEndOf="@+id/firstname_title"
            app:layout_constraintHorizontal_bias="0.903"
            app:layout_constraintStart_toStartOf="@+id/firstname_title"
            app:layout_constraintTop_toBottomOf="@+id/firstname_title" />

        <TextView
            android:id="@+id/password_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/italiana"
            android:text="Password"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/email_title"
            app:layout_constraintHorizontal_bias="0.844"
            app:layout_constraintStart_toStartOf="@+id/email_title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.606" />

        <View
            android:id="@+id/passwordtypebg"
            android:layout_width="326dp"
            android:layout_height="61dp"
            android:background="@drawable/logintypebg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/emailtypebg"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/emailtypebg"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.655" />

        <EditText
            android:id="@+id/password_text"
            android:layout_width="286dp"
            android:layout_height="54dp"
            android:layout_marginTop="75dp"
            android:backgroundTint="#852C42"
            android:ems="10"
            android:fontFamily="@font/italiana"
            android:hint="type here"
            android:inputType="textPassword"
            android:textAlignment="textEnd"
            android:textColor="#FFFFFF"
            android:textColorHint="#D6D5D5"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/email_text"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/email_text"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.63" />

        <EditText
            android:id="@+id/email_text"
            android:layout_width="287dp"
            android:layout_height="48dp"
            android:layout_marginTop="4dp"
            android:backgroundTint="#852C42"
            android:ems="10"
            android:fontFamily="@font/italiana"
            android:hint="type here"
            android:inputType="text"
            android:textAlignment="textEnd"
            android:textColor="#FFFFFF"
            android:textColorHint="#D6D5D5"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/email_text2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/email_text2"
            app:layout_constraintTop_toBottomOf="@+id/phonenumtypebg"
            app:layout_constraintVertical_bias="0.107" />

        <EditText
            android:id="@+id/lastname_text"
            android:layout_width="287dp"
            android:layout_height="48dp"
            android:layout_marginTop="-17dp"
            android:backgroundTint="#852C42"
            android:ems="10"
            android:fontFamily="@font/italiana"
            android:hint="type here"
            android:inputType="text"
            android:textAlignment="textEnd"
            android:textColor="#FFFFFF"
            android:textColorHint="#D6D5D5"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/firstname_text"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/firstname_text"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.331" />

        <EditText
            android:id="@+id/firstname_text"
            android:layout_width="287dp"
            android:layout_height="48dp"
            android:layout_marginTop="6dp"
            android:backgroundTint="#852C42"
            android:ems="10"
            android:fontFamily="@font/italiana"
            android:hint="type here"
            android:inputType="text"
            android:textAlignment="textEnd"
            android:textColor="#FFFFFF"
            android:textColorHint="#D6D5D5"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/firstname_title"
            app:layout_constraintHorizontal_bias="0.988"
            app:layout_constraintStart_toStartOf="@+id/firstname_title"
            app:layout_constraintTop_toBottomOf="@+id/login_button"
            app:layout_constraintVertical_bias="0.035" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="431dp"
            android:layout_height="245dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/submitlogin"
            app:layout_constraintHorizontal_bias="0.538"
            app:layout_constraintStart_toStartOf="@+id/submitlogin"
            app:layout_constraintTop_toBottomOf="@+id/submitlogin"
            app:srcCompat="@drawable/dogplaying" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>